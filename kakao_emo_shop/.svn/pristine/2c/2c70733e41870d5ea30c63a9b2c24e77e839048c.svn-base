package com.itwill.cart;

public class Cart {
	private int car_item_no;
	private int car_item_qty;
	private int m_no;
	private int p_no;
	private String m_niname;
	
	
	public Cart() {
		
	}
	
	public Cart(int car_item_no, int car_item_qty, int m_no) {
		super();
		this.car_item_no = car_item_no;
		this.car_item_qty = car_item_qty;
		this.m_no = m_no;
	}

	public Cart(int car_item_no, int car_item_qty, int m_no, int p_no, String m_niname) {
		super();
		this.car_item_no = car_item_no;
		this.car_item_qty = car_item_qty;
		this.m_no = m_no;
		this.p_no = p_no;
		this.m_niname = m_niname;
	}

	
	public int getCar_item_no() {
		return car_item_no;
	}


	public void setCar_item_no(int car_item_no) {
		this.car_item_no = car_item_no;
	}


	public int getCar_item_qty() {
		return car_item_qty;
	}


	public void setCar_item_qty(int car_item_qty) {
		this.car_item_qty = car_item_qty;
	}


	public int getM_no() {
		return m_no;
	}


	public void setM_no(int m_no) {
		this.m_no = m_no;
	}


	public int getP_no() {
		return p_no;
	}


	public void setP_no(int p_no) {
		this.p_no = p_no;
	}


	public String getM_niname() {
		return m_niname;
	}


	public void setM_niname(String m_niname) {
		this.m_niname = m_niname;
	}

	@Override
	public String toString() {
		return "Cart [car_item_no=" + car_item_no + ", car_item_qty=" + car_item_qty + ", m_no=" + m_no + ", p_no="
				+ p_no + ", m_niname=" + m_niname + "]";
	}
	
	
	
		
}
